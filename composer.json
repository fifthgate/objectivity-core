{
    "name": "fifthgate/objectivity-core",
    "description": "A simple library providing domain-driven design objects for Laravel to use in other projects.",
    "homepage": "https://github.com/fifthgate/objectivity-core",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Baynham",
            "email": "sam@fifthgate.net"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.2",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^8.21",
        "orchestra/testbench-browser-kit": "^8.5",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Fifthgate\\Objectivity\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fifthgate\\Objectivity\\Core\\Tests\\": "tests",
            "Fifthgate\\Objectivity\\Core\\Tests\\Mocks\\": "tests/mocks"
        }
    },
    "config": {
        "platform": {
            "php": "8.3"
        }
    }
}
